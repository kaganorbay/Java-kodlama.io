package intro;

public class Loops {

	public static void main(String[] args) {
		
		for (int i = 0; i < 10; i=i+1) {
			
			System.out.println(i);
			
		}
		
		int i=1;
		while (i<10) {
			System.out.println(i);
			i++;
		}
		int j=2;
		do {
			System.out.println(j);
			j+=2;
		} while (j<10);

	}

}
