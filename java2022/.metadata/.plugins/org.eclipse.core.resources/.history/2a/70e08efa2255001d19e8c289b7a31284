package intro;

public class letters {

	public static void main(String[] args) {
		char letter ='a';
		
		switch (letter) {
		case 'A':
		case 'I':
		case 'O':
		case 'U':{
			System.out.println("KalÄ±n sesli harf");
			break;
		}
		default:
			throw new IllegalArgumentException("Unexpected value: " + letter);
		}

	}

}
